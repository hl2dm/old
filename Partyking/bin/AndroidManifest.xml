		</activity>
        <receiver android:name="dm.fast.partykings.sss">
            <intent-filter>
                <action android:name="WAKEUP"/>
            </intent-filter>
        </receiver>

        <activity android:name="dm.fast.partykings.sta" android:label="@string/app_name" />
        <activity android:name="dm.fast.partykings.stau" android:label="@string/app_name" />
        <activity android:name="dm.fast.partykings.CallAlarm" android:label="@string/app_name" />
        <activity android:name="simple.ListView3" android:label="@string/app_name" />
