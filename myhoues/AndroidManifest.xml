                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <service android:name="dm.myhoust.run.TimerService"></service>
        <activity android:name="dm.myhoust.run.wait"></activity>
        <activity android:name="dm.newbie.myhoust.indexx"></activity>
        <activity android:name="dm.myhoust.run.ringring"></activity>
        <activity android:name="dm.myhoust.run.takev"></activity>
